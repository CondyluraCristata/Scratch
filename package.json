    "@mui/material": "^5.15.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pixi.js": "^7.3.2",
    "pg-promise": "^11.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.21.0",
    "react-router-dom": "^6.21.1",
    "smooth-scroll": "^16.1.3"
  },
